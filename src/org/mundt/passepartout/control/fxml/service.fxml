<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml" type="javafx.scene.control.TitledPane" animated="false">
    <VBox fx:id="mainContainer">
        <ScrollPane xmlns:fx="http://javafx.com/fxml/1"
                    fitToHeight="true"
                    fitToWidth="true"
                    VBox.vgrow="ALWAYS"
                    style="-fx-background-color: transparent;"
                    xmlns="http://javafx.com/javafx/8.0.60">
            <VBox fx:id="formEntriesContainer" spacing="20.0">
                <padding>
                    <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
                </padding>
            </VBox>
        </ScrollPane>
        <ButtonBar>
            <buttons>
                <Button onAction="#launch" text="Launch"/>
            </buttons>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </ButtonBar>
    </VBox>
</fx:root>